{
  "name": "@lagless/circle-race-simulation",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@lagless/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "sourceRoot": "circle-race/circle-race-simulation/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "circle-race/circle-race-simulation/dist",
          "main": "circle-race/circle-race-simulation/src/index.ts",
          "tsConfig": "circle-race/circle-race-simulation/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      }
    }
  },
  "dependencies": {
    "@swc/helpers": "~0.5.11",
    "@lagless/core": "workspace:*",
    "@lagless/math": "workspace:*",
    "@lagless/binary": "workspace:*"
  }
}
