{
  "name": "@lagless/math",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/math/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsup --config tsup.config.ts && tsc -p tsconfig.types.json",
        "cwd": "{projectRoot}"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsup --config tsup.config.ts --watch"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rm -rf dist"
      }
    }
  },
  "tags": []
}
